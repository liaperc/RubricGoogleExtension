{
    "name": "Nueva Canvas to Rubric Extension",
    "version": "0.1.0",
    "manifest_version": 3,
    "key": "",
    "description": "TBD",
    "permissions": [
        "activeTab",
        "storage",
        "identity",
        "downloads"
    ],
    "oauth2": {
        "client_id": "936936943371-5rtqp2midjvldslchtftsk88esf5qlse.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/drive"
        ]
    },
    "host_permissions": [
        "https://*.instructure.com/*",
        "https://*.canvas-user-content.com/*",
        "https://www.googleapis.com/*"
    ],
    "background": {
        "service_worker": "src/background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/content.js"],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_popup": "src/popup/popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png"
        },
        "default_title": "Click to download all of the rubrics as pdfs"
    }
}